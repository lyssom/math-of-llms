<mxfile host="app.diagrams.net">
  <diagram name="Autoregressive Chain Rule">
    <mxGraphModel dx="1200" dy="600" grid="1" gridSize="10" guides="1"
                  tooltips="1" connect="1" arrows="1" fold="1"
                  page="1" pageScale="1" pageWidth="1600" pageHeight="400">
      <root>
        <mxCell id="0"/>
        <mxCell id="1" parent="0"/>

        <!-- x1 -->
        <mxCell id="x1" value="p(x₁)"
                style="rounded=1;whiteSpace=wrap;html=1;fillColor=#dae8fc;strokeColor=#6c8ebf;fontSize=16"
                vertex="1" parent="1">
          <mxGeometry x="40" y="160" width="140" height="60" as="geometry"/>
        </mxCell>

        <!-- x2 -->
        <mxCell id="x2" value="p(x₂ | x₁)"
                style="rounded=1;whiteSpace=wrap;html=1;fillColor=#e1d5e7;strokeColor=#9673a6;fontSize=16"
                vertex="1" parent="1">
          <mxGeometry x="240" y="160" width="180" height="60" as="geometry"/>
        </mxCell>

        <!-- dots -->
        <mxCell id="dots" value="⋯"
                style="text;html=1;fontSize=28;align=center;verticalAlign=middle"
                vertex="1" parent="1">
          <mxGeometry x="470" y="165" width="60" height="50" as="geometry"/>
        </mxCell>

        <!-- xT -->
        <mxCell id="xT" value="p(x_T | x₁,…,x_{T−1})"
                style="rounded=1;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;fontSize=16"
                vertex="1" parent="1">
          <mxGeometry x="580" y="160" width="260" height="60" as="geometry"/>
        </mxCell>

        <!-- arrows -->
        <mxCell id="e1" style="endArrow=block;html=1;strokeWidth=2"
                edge="1" parent="1" source="x1" target="x2">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <mxCell id="e2" style="endArrow=block;html=1;strokeWidth=2"
                edge="1" parent="1" source="x2" target="dots">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <mxCell id="e3" style="endArrow=block;html=1;strokeWidth=2"
                edge="1" parent="1" source="dots" target="xT">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <!-- annotation -->
        <mxCell id="note" value="p(x₁,…,x_T)=∏_{t=1}^T p(x_t | x_{&lt;t})"
                style="text;html=1;fontSize=18;align=center;verticalAlign=middle"
                vertex="1" parent="1">
          <mxGeometry x="300" y="260" width="360" height="40" as="geometry"/>
        </mxCell>

      </root>
    </mxGraphModel>
  </diagram>
</mxfile>
